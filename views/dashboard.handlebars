<div class="row">
  <div class="dashboard">
    <h2>Welcome, {{userData.name}}!</h2>
  </div>
</div>
<h1 class="review-questions">List the address where you parked</h1>
<form id="review-input">
  <input id="text-area" name="text-area" form="review-input"></input>
  <button id="review-questions" type="submit">Submit</button>
</form>
<h1 class="car-type">What type of car do you have?</h1>
<form id="review-input">
  <input id="text-area" name="text-area" form="review-input"></input>
  <button id="review-btn" type="submit">Submit</button>
</form>
<h1 class="car-size">What size of car do you have?</h1>
  <label for="cars">Choose a car size:</label>
  <select name="cars" id="cars">
    <option value="compact">Compact</option>
    <option value="mid-size">Mid-Size</option>
    <option value="full-size">Full-Size</option>
    <option value="suv">SUV</option>
    <option value="truck">Truck</option>
  </select>
  <br><br>
  <input type="submit" id="review-btn" value="Submit">
</form>
<h1 class="review-questions">List any attractions nearby</h1>
<form id="review-input">
  <input id="text-area" name="text-area" form="review-input"></input>
  <button id="review-questions" type="submit">Submit</button>
</form>
<h1 class="review-questions">Write your full review here</h1>
<form id="review-input">
  <textarea id="text-area" name="text-area" rows="10" cols="100" form="review-input"></textarea>
  <button id="review-questions" type="submit">Submit</button>
</form>



<script src="./js/review.js"></script>