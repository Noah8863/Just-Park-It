{{!-- map reviews --}}
{{#each reviews as |review| }}
    <div class="nav-body">
      <div class="navigation">
        <ul>
          <li class="list active">
            <a href="/">
              <span class="icon">
                <i class="fa-solid fa-house"></i>
              </span>
              <span class="text">Home</span>
            </a>
          </li>
          <li class="list">
            <a href="/dashboard">
              <span class="icon">
                <i class="fa-solid fa-square-plus"></i>
              </span>
              <span class="text">New Post</span>
            </a>
          </li>
          <li class="list">
            <a href="#">
              <span class="icon">
                <i class="fa-solid fa-user"></i>
              </span>
              <span class="text">Profile</span>
            </a>
          </li>
          <li class="list">
            <a href="https://github.com/Noah8863/Just-Park-It" target="_blank">
              <span class="icon">
                <i class="fa-solid fa-address-book"></i>
              </span>
              <span class="text">Contact</span>
            </a>
          </li>
          <li class="list">
            <a href="/login">
              <span class="icon">
                <i class="fa-solid fa-person-walking-arrow-right"></i>
              </span>
              <span class="text">Logout</span>
            </a>
          </li>
          {{!-- <div class="indicator"></div> --}}
        </ul>
      </div>
    </div>
<div class="newsfeedContainer">
      <div class="newsfeed">
        <h1 id="title">{{Title}} This is where the Title Goes</h1>
        <h2>{{review.name}}</h2>
        <a>{{format_date review.date_created}} </a> <br>
        {{!-- If we have time lets add a href to the anchor tag so the address will be clickable --}}
        <a id="addressContainer">Address Here</a>
        <p id="sizeOfCarContainer">Size of Car Here</p>
        <p id="typeOfCarContainer">Type of Car Here</p>
        <p class="postInfo">
          {{review.description}}
        </p>
      </div>
    </div>
  </div>
  {{/each}}
  <script src="./js/homepage.js"></script>
  <script src="https://kit.fontawesome.com/78de80bbe9.js" crossorigin="anonymous"></script>